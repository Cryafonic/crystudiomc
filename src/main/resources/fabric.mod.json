{
  "schemaVersion": 1,
  "id": "crystudiomod",
  "version": "${version}",

  "name": "Crystudio Mod",
  "description": "Don't you always just want a feature in your favourite minecraft game. Then the developers says hell no. Well now we will do it our self's.",
  "authors": [
    "Brendon",
    "Gavin"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.crystudio.crystudiomod.CrystudioMod"
    ],
    "client": [
      "net.crystudio.crystudiomod.CrystudioModClient"
    ]
  },
  "mixins": [
    "crystudiomod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.18",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
